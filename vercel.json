{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/listings"
    }
  ],
  "routes": [
    {
      "src": "^/listings(?:/.*)?$",
      "dest": "api/listings.js"
    },
    {
      "src": "^/(?:login|register)$",
      "dest": "api/listings.js"
    }
  ]
}
